    "        id = str(id.strip())\n",
    "        add = 'http://rest.kegg.jp/get/'+id+'/aaseq'\n",
    "        cline = subprocess.Popen(f'wget -c {add} -O {id}',env = None,shell=True)\n",
    "        cline.wait()"
   ]
